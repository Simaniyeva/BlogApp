@{
    ViewData["Title"] = "Index";
}
@model IDataResult<List<TagGetDto>>
<div class="content-header">
    <div>
        <h2 class="content-title card-title">Tags</h2>
        <p>Add, edit, or delete tags</p>
    </div>
    <div>
        <div class="d-grid">
            <a class="btn btn-primary" asp-action="Create" asp-controller="Tag">Add Tag</a>
        </div>
    </div>
</div>
<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive">
					<partial name="_tagTablePartial" model="Model.Data" />
                </div>
            </div>
        </div> 
    </div> 
</div>